{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/tweets.schema.json",
  "title": "Twitter - Tweets",
  "description": "Tweets of Twitter account.",
  "type": "object",
  "properties": {
    "account_name": {
      "type": "string",
      "description": "Twitter account name"
    },
    "timestamp": {
      "type": "string",
      "description": "When tweet was posted."
    },
    "content": {
      "type": "string",
      "description": "Content of tweet."
    },
    "favourites": {
      "type": "number",
      "description": "Count of favourites."
    },
    "retweets": {
      "type": "number",
      "description": "Count of retweets."
    },
    "likes": {
      "type": "number",
      "description": "Count of likes."
    }
  }
}
