{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/twitter-growth.schema.json",
  "title": "Twitter - Growth",
  "description": "Growth metrics of Twitter account.",
  "type": "object",
  "properties": {
    "account_name": {
      "type": "string",
      "description": "Twitter account name"
    },
    "date": {
      "type": "string",
      "description": "Date metric was recorded."
    },
    "followers": {
      "type": "number",
      "description": "Count of followers."
    },
    "following": {
      "type": "number",
      "description": "Count of following."
    },
    "tweets": {
      "type": "number",
      "description": "Count of tweets."
    },
    "favourites": {
      "type": "number",
      "description": "Count of favourites."
    }
  }
}
