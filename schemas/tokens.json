{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/tokens.schema.json",
  "title": "Tokens",
  "description": "Information about NFT collection.",
  "type": "object",
  "properties": {
    "collection_name": {
      "type": "string",
      "description": "Name of NFT collection."
    },
    "contract_address": {
      "type": "string",
      "description": "Address of NFT contract."
    },
    "token_id": {
      "type": "string",
      "description": "ID of token."
    },
    "token_symbol": {
      "type": "string",
      "description": "Symbol of token."
    },
    "token_uri": {
      "type": "string",
      "description": "URI of token."
    },
    "token_metadata": {
      "type": "string",
      "description": "Metadata of token."
    },
    "contract_type": {
      "type": "string",
      "description": "Type of contract."
    },
    "block_minted": {
      "type": "object",
      "description": "Block number when minted."
    }
  }
}
