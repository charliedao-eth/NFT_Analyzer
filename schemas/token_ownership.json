{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/token-ownership.schema.json",
  "title": "Tokens - Ownership",
  "description": "Number of tokens owned by owner.",
  "type": "object",
  "properties": {
    "collectionName": {
      "type": "string",
      "description": "NFT collection name."
    },
    "ownerAddress": {
      "type": "string",
      "description": "Address of owner."
    },
    "tokenCount": {
      "type": "number",
      "description": "Count of owned tokens."
    }
  }
}
